*/

/* *************** INPUT PINS *********************/


PIN  1 = C3;
PIN  2 = A15pal2;
PIN  3 = nCE;
PIN  4 = nMREQpal2;
PIN  5 = Q6;
PIN  6 = nRD;
PIN  7 = Q4;
PIN  8 = Q5;
PIN  9 = Q3;
PIN 11 = 640K;

/* *************** OUTPUT PINS *********************/

PIN 12 = A18;
PIN 13 = A16;
PIN 14 = nRDout;
PIN 15 = nCE2;
PIN 16 = A17;
PIN 17 = nMREQout;
PIN 18 = nCE1;
PIN 19 = A15out;



nCE1 = !( !nCE &  Q6 & !C3);


nCE2 = !( !nCE & !Q6 & !C3
	    # nCE & !nMREQpal2 &  nRD
	    # nCE & !nMREQpal2 & !nRD & C3 ) ;


A15out = A15pal2
	#  nCE & C3 & !nMREQpal2;
/* OKAY??? */

A16 =     nCE
        # !C3 & Q3 & !Q4 &  Q5 & !640K
        # !C3 & Q3 &  Q4 & !Q5 & !640K
        # !C3 & Q3 & !Q4 & !Q5 & !640K
;

A17 =     nCE  
	 # 640K
        # !C3 & Q4 & !Q3 &  Q5 & !640K
        # !C3 & Q4 &  Q3 & !Q5 & !640K
        # !C3 & Q4 & !Q3 & !Q5 & !640K
;

A18 =     nCE & !640K
        # !C3 & Q5 & !Q3 &  Q4 & !640K
        # !C3 & Q5 &  Q3 & !Q4 & !640K
        # !C3 & Q5 & !Q3 & !Q4 & !640K
;

nMREQout = !( !nMREQpal2 & nRD
	     # !nMREQpal2 & !nRD & !C3);

nRDout   = ! (!nRD & nMREQpal2 
	 	# !nMREQpal2 & !nRD & !C3);












